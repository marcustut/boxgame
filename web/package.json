{
  "name": "thebox",
  "version": "0.0.1",
  "scripts": {
    "format": "prettier --write src",
    "lint": "eslint --ext .ts,.tsx src",
    "dev": "yarn server",
    "prod": "yarn build && yarn server:prod",
    "build": "vite build && vite build --ssr",
    "build:ssg": "vite build && vite build --ssr && vite-plugin-ssr prerender",
    "server": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "supabase:generate-types": "npx openapi-typescript \"$(grep VITE_SUPABASE_URL .env.local | cut -d '=' -f2 | cut -d '\"' -f 2)/rest/v1/?apikey=$(grep VITE_SUPABASE_ANON_KEY .env.local | cut -d '=' -f2 | cut -d '\"' -f 2)\" --output src/types/supabase.ts",
    "graphql:generate-types": "apollo client:codegen --localSchemaFile=../internal/graphql/schema.graphqls --target=typescript --tagName=gql --outputFlat src/graphql/types --passthroughCustomScalars --customScalarsPrefix=TheBox."
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@date-io/dayjs": "^2.11.0",
    "@emotion/react": "11.4.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "11.3.0",
    "@headlessui/react": "^1.4.1",
    "@mui/icons-material": "^5.0.1",
    "@mui/lab": "^5.0.0-alpha.50",
    "@mui/material": "^5.0.1",
    "@react-three/drei": "^7.16.0",
    "@react-three/fiber": "^7.0.16",
    "@supabase/supabase-js": "^1.24.0",
    "@types/react-highlight-words": "^0.16.3",
    "blobity": "^0.1.7",
    "cross-fetch": "^3.1.4",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "emoji-mart": "^3.0.1",
    "express": "^4.17.1",
    "formik": "^2.2.9",
    "framer-motion": "^4.1.17",
    "glsl-noise": "^0.0.0",
    "glslify": "^7.1.1",
    "graphql": "^15.6.0",
    "history": "^5.0.1",
    "immer": "^9.0.6",
    "notistack": "^2.0.2",
    "react": "^17.0.0",
    "react-countdown": "^2.3.2",
    "react-dom": "^17.0.0",
    "react-error-boundary": "^3.1.3",
    "react-helmet-async": "^1.1.2",
    "react-highlight-words": "^0.17.0",
    "react-router-dom": "6.0.0-beta.5",
    "react-use": "^17.3.1",
    "react-waypoint": "^10.1.0",
    "rebound": "^0.1.0",
    "three": "^0.133.1",
    "ts-node": "^10.2.1",
    "vite-plugin-ssr": "^0.3.9",
    "yup": "^0.32.9",
    "zustand": "^3.5.12"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@iconify/react": "^3.0.1",
    "@types/emoji-mart": "^3.0.8",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/node": "^16.10.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/three": "^0.133.1",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "apollo": "^2.33.6",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "rc": "^1.2.8",
    "react-scripts": "^4.0.3",
    "typescript": "^4.1.2",
    "vite": "^2.6.3",
    "vite-plugin-checker": "^0.3.4",
    "vite-plugin-windicss": "^1.4.9",
    "windicss": "^3.1.9"
  },
  "resolutions": {
    "graphql": "^15.6.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
